      <Query>
        <DataSourceName>DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select v.vendid, v.invcdate, v.invcnbr, po.buyer, po.vendname, po.ponbr, po.podate, t.trandate, t.trandesc, t.PerEnt, t.PerPost
	from APDoc v, PurchOrd po, APTran t
	where v.VendId = po.VendID 
		and v.BatNbr = t.BatNbr
		and v.RefNbr = v.RefNbr 
		and v.InvcNbr &lt;&gt; ''
		and po.PODate between '2011-12-01' and '2011-12-1'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
