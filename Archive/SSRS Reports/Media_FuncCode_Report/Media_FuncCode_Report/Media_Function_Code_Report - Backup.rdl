        </Field>
        <Field Name="Debit">
          <DataField>Debit</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQL1_DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>--- final report so far
select b.[Function Code]
, b.[Function Code Desc]
--, a.[Function Code Type]
, a.[Function Group]
, a.[Function Group Desc]
, b.Project
, b.[Project Desc]
, b.Acct
, b.[Acct Desc]
, b.[Sub Acct]
, b.[Sub Acct Desc]
, b.PerEnt
, b.PerPost
, b.Credit
, b.Debit
from 
(select code_value, code_type as 'Function Code Type'
	, code_value as 'Function Group'
	, code_value_desc as 'Function Group Desc'
		from PJCODE where code_type = '9FCG')a,
	(select f.code_group
	, gl.TaskID as 'Function Code'
	, f.descr as 'Function Code Desc'
	, gl.ProjectID as 'Project'
	, p.project_desc as 'Project Desc'
	, gl.Acct as 'Acct' 
	, a.Descr as 'Acct Desc' 
	, gl.[SUB] as 'Sub Acct' 
	, sa.Descr as 'Sub Acct Desc' 
	, gl.PerEnt
	, gl.PerPost
	, gl.cramt as 'Credit'
	, gl.dramt as 'Debit'
		from GLTran gl, Account a, SubAcct sa, xIGFunctionCode f, PJPROJ p
			where gl.acct = a.acct 
			AND gl.[Sub] = sa.[Sub] 
			AND gl.TaskID = f.code_ID
			AND gl.ProjectID = p.project
			--AND rtrim(gl.fiscyr) = @fiscyr 
			AND rtrim(gl.PerPost) = @perPost
			AND rtrim(gl.Acct) &lt;= '4970' 
			AND rtrim(gl.Acct) &gt;= '4700')b 
where b.code_group = a.code_value
and b.[Function Code] IN (@sFuncCode)
--and b.[Function Code] IN (select distinct(TaskID) from GLTran where Acct between '4700' and '4970' and TaskID &lt;&gt; '')
order by b.[Function Code]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@perPost">
            <Value>=Parameters!perPost.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@sFuncCode">
            <Value>=Parameters!sFuncCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
