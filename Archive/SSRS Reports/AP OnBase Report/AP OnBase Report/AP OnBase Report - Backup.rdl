      <Query>
        <DataSourceName>SQL1_DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>-- SSRS Report Query Starts Here
declare @begTime datetime
declare @endTime datetime
declare @begin datetime
declare @end datetime
set @begTime = '12:00:00 AM'
set @endTime = '11:59:59 PM'
set @begin = Convert(datetime, Convert(char(10), @begDate, 103) + ' ' + Convert(char(8), @begTime, 108), 103)
set @end = Convert(datetime, Convert(char(10), @endDate, 103) + ' ' + Convert(char(8), @endTime, 108), 103)

select a.* from 
(select 'DENVER' as Company 
, ap.RefNbr
, ap.InvcNbr
, ap.InvcDate
, ap.Crtd_DateTime as DateCreated
, ap.vendid
, v.Name as VendorName
, ap.PONbr
, ap.CuryOrigDocAmt as InvcAmount
, ap.User2 as OnBaseNum
from SQL1.DENVERAPP.DBO.APDOC ap 
left outer join 
SQL1.DENVERAPP.DBO.VENDOR v on ap.VendId = v.VendId
where ap.Crtd_DateTime between @begin and @end
and DocType = 'VO'
UNION All
select 'MIDWEST' as Company 
, ap.RefNbr
, ap.InvcNbr
, ap.InvcDate
, ap.Crtd_DateTime as DateCreated
, ap.vendid
, v.Name as VendorName
, ap.PONbr
, ap.CuryOrigDocAmt as InvcAmount
, ap.User2 as OnBaseNum
from SQL1.MIDWESTAPP.DBO.APDOC ap 
left outer join 
SQL1.MIDWESTAPP.DBO.VENDOR v on ap.VendId = v.VendId
where ap.Crtd_DateTime between @begin and @end
and DocType = 'VO'
UNION All
select 'NEWYORK' as Company 
, ap.RefNbr
, ap.InvcNbr
, ap.InvcDate
, ap.Crtd_DateTime as DateCreated
, ap.vendid
, v.Name as VendorName
, ap.PONbr
, ap.CuryOrigDocAmt as InvcAmount
, ap.User2 as OnBaseNum
from SQL1.NEWYORKAPP.DBO.APDOC ap 
left outer join 
SQL1.NEWYORKAPP.DBO.VENDOR v on ap.VendId = v.VendId
where ap.Crtd_DateTime between @begin and @end
and DocType = 'VO'
UNION All
select 'LA' as Company 
, ap.RefNbr
, ap.InvcNbr
, ap.InvcDate
, ap.Crtd_DateTime as DateCreated
, ap.vendid
, v.Name as VendorName
, ap.PONbr
, ap.CuryOrigDocAmt as InvcAmount
, ap.User2 as OnBaseNum
from SQL1.LAAPP.DBO.APDOC ap 
left outer join 
SQL1.LAAPP.DBO.VENDOR v on ap.VendId = v.VendId
where ap.Crtd_DateTime between @begin and @end
and DocType = 'VO'
)a
where a.Company = @sCompany
order by a.DateCreated</CommandText>
