  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Func_Grp">
          <DataField>Func_Grp</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Func_Grp_Desc">
          <DataField>Func_Grp_Desc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Func_Code">
          <DataField>Func_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Func_Code_Desc">
          <DataField>Func_Code_Desc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Current_Actual">
          <DataField>Current_Actual</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Budget">
          <DataField>Budget</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="YTD_Total">
          <DataField>YTD_Total</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Variance">
          <DataField>Variance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Eventive</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>-- For internal reporting use
DECLARE @CurrentDate Date	--Would have been used in used in WIP AGING logic
DECLARE @iYear as int
SET @CurrentDate = getdate()
SET @iYear = YEAR(@CurrentDate)


--Eventive PNC Report Query
select 
a.Func_Grp
, a.Func_Grp_Desc
, a.Func_Code
, a.Func_Code_Desc
, SUM(a.Current_Actual) as Current_Actual
, SUM(a.Budget) as Budget
, SUM(a.YTD_Total) as YTD_Total
, (SUM(a.Budget) - SUM(a.YTD_Total)) as Variance
from
(select cg.code_value as 'Func_Grp'
, cg.code_value_desc  as 'Func_Grp_Desc'
, fc.Account as 'GL_Acct'
, fc.code_ID as 'Func_Code'
, fc.descr as 'Func_Code_Desc'
, p.customer
, c.Name
, pjt.project
--, pjt.tr_comment
, isnull(pjt.amount,0) as 'Current_Actual'
, isnull(cle.CLEAmount,0) as 'Budget'		-- This is the Current locked Estimate
, isnull(fjr.Actuals,0) as 'YTD_Total'	-- These are the year to dat actuals
--, *
from PJCODE cg 
LEFT OUTER JOIN xIGFunctionCode fc ON cg.code_value = fc.code_group
INNER JOIN PJTran pjt ON fc.code_ID = pjt.pjt_entity
LEFT OUTER JOIN PJPROJ p ON pjt.project = p.project
LEFT OUTER JOIN Customer c ON p.customer = c.CustId
LEFT OUTER JOIN --FJR query.  To get down to one line for reporting pulling as main source
						( SELECT m.Project
						, SUM ( CASE WHEN m.AcctGroupCode IN ('WA','WP','CM','FE') THEN
								AmountBF + Amount01 + Amount02 + Amount03 + Amount04 + Amount05 + Amount06 + Amount07 + Amount08 + Amount09 + Amount10 + Amount11 + Amount12 + Amount13 + Amount14 + Amount15
								ELSE 0  END ) as Actuals
							From xvr_PA924_Main m 
							Where m.FSYearNum = @iYear 
							Group by m.Project ) FJR ON p.project = FJR.Project
LEFT OUTER JOIN /*(Using Current Locked Estimate view with functions.  Must roll up to project or cartestion joins */
			( Select CLE.Project 
				, SUM ( CASE WHEN (FC.code_group = 'FEE' and FC.code_ID &lt;&gt; '00975') THEN CLE.CLEAmount ELSE 0 END ) as CLEFee
				, SUM (CLE.CLEAmount) as CLEAmount 
			   From xvr_Est_CLE CLE JOIN xIGFunctionCode FC ON CLE.pjt_entity = FC.code_ID
			   Group by CLE.Project ) CLE ON p.Project = CLE.Project
where  
--and 
fc.code_ID IS NOT NULL 
and 
pjt.acct = 'BILLABLE'
and code_type = '9FCG'
and pjt.fiscalno =@fiscalno) a
group by a.Func_Grp, a.Func_Grp_Desc, a.Func_Code, a.Func_Code_Desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@fiscalno">
            <Value>=Parameters!fiscalno.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="fiscalno">
      <DataType>String</DataType>
      <Prompt>fiscalno</Prompt>
    </ReportParameter>
  </ReportParameters>
</Report>