      </Fields>
      <Query>
        <DataSourceName>SQL1_DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>Select a.* from
(select 'DENVER' as Company, vendid, name as vendName, classid, Terms from DENVERAPP.dbo.vendor 
UNION ALL
select 'MIDWEST' as Company, vendid, name as vendName, classid, Terms from MIDWESTAPP.dbo.vendor 
UNION ALL
select 'NEWYORK' as Company, vendid, name as vendName, classid, Terms from NEWYORKAPP.dbo.vendor
UNION ALL
select 'LA' as Company, vendid, name as vendName, classid, Terms from LAAPP.dbo.vendor)a
where a.Company = @sCompany</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@sCompany">
            <Value>=Parameters!sCompany.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
