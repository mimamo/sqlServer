      <Query>
        <DataSourceName>SQL1_DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>--dsByBU_w_YTD
declare @intMonth int

if (MONTH(GETDATE())=12) 
	set @intMonth = 12
else
	set @intMonth = MONTH(GETDATE())-1


select a.BusinessUnit
, a.Department
, a.SalesMarketing
, a.Jan_Act as 'Jan'
, a.Feb_Act as 'Feb'
, a.Mar_Act as 'Mar'
, a.Apr_Act as 'Apr'
, a.May_Act as 'May'
, a.Jun_Act as 'Jun'
, a.Jul_Act as 'Jul'
, a.Aug_Act as 'Aug'
, a.Sep_Act as 'Sep'
, a.Oct_Act as 'Oct'
, a.Nov_Act as 'Nov'
, a.Dec_Act as 'Dec'
, ISNULL(CAST(((a.Jan_Act+a.Feb_Act+a.Mar_Act+a.Apr_Act+a.May_Act+a.Jun_Act+a.Jul_Act+a.Aug_Act+a.Sep_Act+a.Oct_Act+a.Nov_Act+a.Dec_Act)/f.cMonth) as decimal(4,2)),0) as 'YTD'
, ISNULL(CAST((f.fPpl/f.cMonth) as decimal(4,2)),0) as 'Forecst'
, ISNULL(CAST((f.FTE_Adj/f.cMonth) as decimal(4,2)),0) as 'FTE Adjust'
, ISNULL(CAST((f.adj_fPpl/f.cMonth) as decimal(4,2)),0) as 'Adj Forecast'
from
(select d.BusinessUnit 
, d.Department
, d.SalesMarketing
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 1) as decimal(4,2)),0) as 'Jan_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 2) as decimal(4,2)),0) as 'Feb_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 3) as decimal(4,2)),0) as 'Mar_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 4) as decimal(4,2)),0) as 'Apr_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 5) as decimal(4,2)),0) as 'May_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 6) as decimal(4,2)),0) as 'Jun_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 7) as decimal(4,2)),0) as 'Jul_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 8) as decimal(4,2)),0) as 'Aug_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 9) as decimal(4,2)),0) as 'Sep_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 10) as decimal(4,2)),0) as 'Oct_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 11) as decimal(4,2)),0) as 'Nov_Act' 
, ISNULL(CAST((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and SalesMarketing=d.SalesMarketing and CurMonth = 12) as decimal(4,2)),0) as 'Dec_Act' 
from 
xwrk_MC_Data d
group by d.BusinessUnit, d.Department, d.SalesMarketing)a 
LEFT OUTER JOIN 
(select BusinessUnit, Department, SalesMarketing
, SUM(fPpl) as 'fPpl'
, SUM(fte_adj) as 'FTE_Adj'
, SUM(adj_fPpl) as 'adj_fPpl'
, CASE WHEN MONTH(GETDATE()) = 12 THEN 12 ELSE MONTH(GETDATE())-1 END as 'cMonth'
from xwrk_MC_Forecast 
where fMonth &lt;= @intMonth
group by BusinessUnit, Department, SalesMarketing)f 
ON (a.BusinessUnit = f.BusinessUnit and a.Department = f.Department and a.SalesMarketing = f.SalesMarketing)
WHERE a.BusinessUnit NOT LIKE ('OOS%')
order by a.BusinessUnit, a.Department, a.SalesMarketing</CommandText>
