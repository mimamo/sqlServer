      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select *
from 
(select brand, BusinessUnit, Dept2, Title
, CAST((SUM(JanuaryHours)/166.67) as decimal(4,2)) as JanPpl
, CAST((SUM(FebruaryHours)/166.67) as decimal(4,2)) as FebPpl
, CAST((SUM(MarchHours)/166.67) as decimal(4,2)) as MarPpl
, CAST((SUM(AprilHours)/166.67) as decimal(4,2)) as AprPpl
, CAST((SUM(MayHours)/166.67) as decimal(4,2)) as MayPpl
, CAST((SUM(JuneHours)/166.67) as decimal(4,2)) as JunePpl
, CAST((SUM(JulyHours)/166.67) as decimal(4,2)) as JulyPpl
, CAST((SUM(AugustHours)/166.67) as decimal(4,2)) as AugPpl
, CAST((SUM(SeptemberHours)/166.67) as decimal(4,2)) as SeptPpl
, CAST((SUM(OctoberHours)/166.67) as decimal(4,2)) as OctPpl
, CAST((SUM(NovemberHours)/166.67) as decimal(4,2)) as NovPpl
, CAST((SUM(DecemberHours)/166.67) as decimal(4,2)) as DecPpl
, CAST((SUM(total)/2000) as decimal(4,2)) as totalPpl 
from xwrk_CR600 group by brand, BusinessUnit, Dept2, Title)a,
(select brand, BusinessUnit, Department, Title
, CAST(SUM(JanForecast) as decimal(4,2)) as JanForecast
, CAST(SUM(FebForecast) as decimal(4,2)) as FebForecast
, CAST(SUM(MarForecast) as decimal(4,2)) as MarForecast
, CAST(SUM(AprForecast) as decimal(4,2)) as AprForecast
, CAST(SUM(MayForecast) as decimal(4,2)) as MayForecast
, CAST(SUM(JunForecast) as decimal(4,2)) as JuneForecast
, CAST(SUM(JulForecast) as decimal(4,2)) as JulyForecast
, CAST(SUM(AugForecast) as decimal(4,2)) as AugForecast
, CAST(SUM(SepForecast) as decimal(4,2)) as SeptForecast
, CAST(SUM(OctForecast) as decimal(4,2)) as OctForecast
, CAST(SUM(NovForecast) as decimal(4,2)) as NovForecast
, CAST(SUM(DecForecast) as decimal(4,2)) as DecForecast
, CAST(SUM(TotalForecast) as decimal(4,2)) as totalForecast 
from xwrk_MC_Ppl_Forecast group by brand, BusinessUnit, Department, Title)f 
where a.Brand = f.Brand
AND a.BusinessUnit = f.BusinessUnit
AND a.Dept2 = f. Department
AND a.Title = f.Title
order by a.Brand, a.BusinessUnit, a.Dept2, a.Title</CommandText>
