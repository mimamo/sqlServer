n</Width><Page><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="SQL1_DENVERAPP"><DataSourceReference>SQL1_DENVERAPP</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">71dc03ac-f28f-4850-817e-fb40d1f04bf1</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="DataSet1"><Query><DataSourceName>SQL1_DENVERAPP</DataSourceName><CommandText>-- Project report for Nexonia
-- Projects.csv
select ltrim(rtrim(p.project)) as Project
, ltrim(rtrim(p.project_desc)) as ProjectDescripton
, ltrim(rtrim(p.manager1)) as ProjectMgrID
--, LTRIM(rtrim((select em_id01 from PJEMPLOY where employee = p.manager1))) as PMVendID
, CASE WHEN right(rtrim(p.project),3) = 'AGY' OR right(rtrim(p.project),3) = 'IGG' OR right(rtrim(p.project),3) = 'BAL' THEN 'Yes' ELSE 'No' END as PMApproval
, gl_subacct
, 'DENVER' as Company
from DENVERAPP.dbo.PJPROJ p
where status_pa = 'A'
AND status_ap &lt;&gt; 'I'
AND contract_type NOT IN ('TIME','FIN','APS','MED')
UNION ALL
select ltrim(rtrim(p.project)) as Project
, ltrim(rtrim(p.project_desc)) as ProjectDescripton
, ltrim(rtrim(p.manager1)) as ProjectMgrID
--, LTRIM(rtrim((select em_id01 from PJEMPLOY where employee = p.manager1))) as PMVendID
, CASE WHEN right(rtrim(p.project),3) = 'AGY' OR right(rtrim(p.project),3) = 'IGG' OR right(rtrim(p.project),3) = 'BAL' THEN 'Yes' ELSE 'No' END as PMApproval
, gl_subacct
, 'SHOPPERNY' as Company
from SHOPPERAPP.dbo.PJPROJ p
where status_pa = 'A'
AND status_ap &lt;&gt; 'I'
AND contract_type NOT IN ('TIME','FIN','APS','MED')</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="Project"><DataField>Project</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="ProjectDescripton"><DataField>ProjectDescripton</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="ProjectMgrID"><DataField>ProjectMgrID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="PMApproval"><DataField>PMApproval</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Company"><DataField>Company</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="gl_subacct"><DataField>gl_subacct</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet></DataSets><Language>en-US</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">d7d18d8a-2796-4f8b-8c34-494ecd8c16b0</rd:ReportID></Report>