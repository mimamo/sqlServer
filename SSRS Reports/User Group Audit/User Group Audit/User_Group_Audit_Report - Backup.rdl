          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ADate">
          <DataField>ADate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQL1_DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select ASolomonUserID as AdminUser
  , GroupId
  , UserId
  , case when AProcess = 'U' then 'UPDATE'
		when AProcess = 'D' then 'DELETE'
		when AProcess = 'I' then 'INSERT'
	end as Action
  , ADate from DENVERSYS.DBO.xAUserGrp
  where ADate between @sBegDate and @sEndDate
  order by ADate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@sBegDate">
            <Value>=Parameters!sBegDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@sEndDate">
            <Value>=Parameters!sEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
