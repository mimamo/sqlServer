      <Query>
        <DataSourceName>SQL1_DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>-- J&amp;J Monthly Report 
select ltrim(rtrim(cust.CustID)) as 'Client ID'
, ltrim(rtrim(cust.Name)) as 'Customer Name'
, ltrim(rtrim(p.project)) as 'Job'
, ltrim(rtrim(p.project_desc)) as 'Job Description'
, ltrim(rtrim(p.pjt_entity)) as 'Function Code'
, ltrim(rtrim(v.VendId)) as 'Vendor ID'
, ltrim(rtrim(v.Name)) as 'Vendor Name'
, ltrim(rtrim(p.in_id01)) as 'Vendor Invoice Number'
, ltrim(rtrim(t.vendInvDate)) as 'Vendor Invoice Date'
, ltrim(rtrim(p.invoice_num)) as 'Client Invoice Number'
, ltrim(rtrim(p.invoice_date)) as 'Client Invoice Date'
, ltrim(rtrim(p.cost_amt)) as 'Cost'
, ltrim(rtrim(p.gross_amt)) as 'Invoice Amount'
, ltrim(rtrim(chk.AdjgRefNbr)) as 'Check Number'
, ltrim(rtrim(c.docdate)) as 'Check Date'
, ltrim(rtrim(c.CuryOrigDocAmt)) as 'Check Amount'
, ltrim(rtrim(chk.AdjgDocType)) as 'DocType'
 FROM
 (select CustId, Name from Customer 
	where CustId IN ('1LFSC','1LFSU','1JJVC'))cust 
LEFT OUTER JOIN 
(select p.project, p.project_desc, a.invoice_num, a.invoice_date, a.gross_amt, a.project_billwith, a.customer, a.draft_num, b.cost_amt, b.pjt_entity
		, b.vendor_num, b.in_id01 from PJPROJ p 
		left outer join PJINVHDR a ON p.project = a.project_billwith
		left outer join (select acct, cost_amt, pjt_entity, vendor_num, in_id01, draft_num from PJInvDet where ltrim(rtrim(acct)) = 'OFFSET PREBILL'
		UNION ALL
		select acct, cost_amt, pjt_entity, vendor_num, in_id01, draft_num from PJInvDet where ltrim(rtrim(acct)) &lt;&gt; 'OFFSET PREBILL' 
		and draft_num NOT IN (select draft_num from PJInvDet where ltrim(rtrim(acct)) = 'OFFSET PREBILL')) b 
		ON a.draft_num = b.draft_num)p ON cust.CustId = p.customer
 LEFT OUTER JOIN Vendor v ON p.vendor_num = v.VendId 
 LEFT OUTER JOIN (select tr_id02, max(tr_id08) as 'vendInvDate' from PJTran group by tr_id02) t ON p.in_id01 = t.tr_id02
 
 LEFT OUTER JOIN (select adj.AdjgRefNbr , adj.AdjdRefNbr, ap.InvcNbr, ap.VendId, ap.DocDate, ap.CuryOrigDocAmt, adj.AdjgDocType, adj.AdjgDocDate
			from
			(select RefNbr, doctype, InvcNbr, vendid, DocDate, CuryOrigDocAmt from APDoc where doctype IN ('VO','VT'))ap,  
				APAdjust adj 
			where ap.RefNbr = adj.AdjdRefNbr 
			AND adj.AdjgDocType = 'CK'
			AND adj.AdjgRefNbr NOT IN (select adj.AdjgRefNbr from 
				(select RefNbr from APDoc where doctype IN ('VO','VT'))ap,  
				APAdjust adj 
				where ap.RefNbr = adj.AdjdRefNbr 
				AND adj.AdjgDocType = 'VC')
			AND adj.AdjgDocDate &gt; '01/01/2008') chk ON (p.invoice_num = chk.invcnbr and p.vendor_num = chk.VendId)
 
 LEFT OUTER JOIN APDoc c ON chk.AdjgRefNbr = c.RefNbr
 where p.invoice_date between @dBDate and @dEDate
 order by cust.CustId, p.project, p.invoice_num</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@dBDate">
            <Value>=Parameters!dBDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dEDate">
            <Value>=Parameters!dEDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="dBDate">
      <DataType>String</DataType>
      <Prompt>d BDate</Prompt>
    </ReportParameter>
    <ReportParameter Name="dEDate">
      <DataType>String</DataType>
      <Prompt>d EDate</Prompt>
    </ReportParameter>
  </ReportParameters>
</Report>