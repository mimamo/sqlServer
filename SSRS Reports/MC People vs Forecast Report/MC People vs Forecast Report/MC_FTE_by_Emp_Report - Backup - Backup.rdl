      <Query>
        <DataSourceName>SQL1_DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>--dsByBU_to_Emp_w_YTD
declare @intMonth int

if (MONTH(GETDATE())=12) 
	set @intMonth = 12
else
	set @intMonth = MONTH(GETDATE())-1

select a.BusinessUnit
, a.Department
, a.Title
, a.Employee_Name
, CAST(a.Jan_Act as decimal(4,2)) as 'Jan'
, CAST(a.Feb_Act as decimal(4,2)) as 'Feb'
, CAST(a.Mar_Act as decimal(4,2)) as 'Mar'
, CAST(a.Apr_Act as decimal(4,2)) as 'Apr'
, CAST(a.May_Act as decimal(4,2)) as 'May'
, CAST(a.Jun_Act as decimal(4,2)) as 'Jun'
, CAST(a.Jul_Act as decimal(4,2)) as 'Jul'
, CAST(a.Aug_Act as decimal(4,2)) as 'Aug'
, CAST(a.Sep_Act as decimal(4,2)) as 'Sep'
, CAST(a.Oct_Act as decimal(4,2)) as 'Oct'
, CAST(a.Nov_Act as decimal(4,2)) as 'Nov'
, CAST(a.Dec_Act as decimal(4,2)) as 'Dec'
, ISNULL(CAST(((a.Jan_Act+a.Feb_Act+a.Mar_Act+a.Apr_Act+a.May_Act+a.Jun_Act+a.Jul_Act+a.Aug_Act+a.Sep_Act+a.Oct_Act+a.Nov_Act+a.Dec_Act)/a.cMonth) as decimal(4,2)),0) as 'YTD'
from
(select d.BusinessUnit 
, d.Department
, d.Title
, d.Employee_Name
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 1),0) as 'Jan_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 2),0) as 'Feb_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 3),0) as 'Mar_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 4),0) as 'Apr_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 5),0) as 'May_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 6),0) as 'Jun_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 7),0) as 'Jul_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 8),0) as 'Aug_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 9),0) as 'Sep_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 10),0) as 'Oct_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 11),0) as 'Nov_Act' 
, ISNULL((select (SUM(CurHours)/166.67) as CurHours from xwrk_MC_Data where BusinessUnit=d.BusinessUnit and Department=d.Department and title=d.title and Employee_Name = d.Employee_Name and CurMonth = 12),0) as 'Dec_Act' 
, CASE WHEN MONTH(GETDATE()) = 12 THEN 12 ELSE MONTH(GETDATE())-1 END as 'cMonth'
from 
xwrk_MC_Data d
where d.CurMonth &lt;= @intMonth
group by d.BusinessUnit , d.Department, d.Title, d.Employee_Name)a 
WHERE a.BusinessUnit NOT LIKE ('OOS%')
order by a.BusinessUnit, a.Department, a.Title, a.Employee_Name</CommandText>
