  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Brand">
          <DataField>Brand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BusinessUnit">
          <DataField>BusinessUnit</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Department">
          <DataField>Department</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Actual">
          <DataField>Actual</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Forecast">
          <DataField>Forecast</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Variance">
          <DataField>Variance</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select a.Brand
, a.BusinessUnit
, a.dept2 as 'Department'
, a.Title
, a.CurHours as 'Actual'
, f.fPpl as 'Forecast'
, (a.CurHours - f.fPpl) as 'Variance'
from 
(select brand, BusinessUnit, Dept2, Title, CurMonth
, CAST((SUM(CurHours)/166.67) as decimal(4,2)) as CurHours
from xwrk_CR600 group by brand, BusinessUnit, Dept2, Title, curMonth)a,
(select brand, BusinessUnit, Department, Title, fMonth
, CAST(SUM(fPpl) as decimal(4,2)) as fPpl
from xwrk_MC_Forecast group by brand, BusinessUnit, Department, Title, fMonth)f 
where a.Brand = f.Brand
AND a.BusinessUnit = f.BusinessUnit
AND a.Dept2 = f. Department
AND a.Title = f.Title
AND a.CurMonth = f.fMonth
order by a.Brand, a.BusinessUnit, a.Dept2, a.Title, a.CurMonth</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
