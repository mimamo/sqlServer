      <Query>
        <DataSourceName>SQL1_DENVERAPP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>--------- Mojo Data for 3D Retained Hours (Query with project info and ClientID, Brand, BU, subUnit)
select a.Brand
, a.BusinessUnit
, a.SubUnit
, a.Department
, a.[Project Number] as ProjectID
, a.Comments as ProjectDesc
, a.ClientID
, a.[Client Product] as ProductID
, a.Employee_Name
, a.Title
, a.SalesMarketing
, a.curMonth
, a.[Actual Hours Worked] as CurHours
, a.Year
, 100 * a.Year + a.curMonth as fiscalno 
--, CASE WHEN a.curMonth &lt; 10 THEN ''+a.Year+'0'+a.curMonth else ''+a.Year+a.curMonth end as 'fiscalno'
from
(select
p.pm_id01 as 'ClientID'
, RTRIM(xwrk_MC_Brd_Grp.brand) as 'Brand'
, RTRIM(xwrk_MC_Brd_Grp.businessUnit) as 'BusinessUnit'
, RTRIM(xwrk_MC_Brd_Grp.subUnit) as 'SubUnit'
, 'Creative' as 'Department'
, m.[Client Division]
, m.[Client Name]
, m.[Project Type]
, m.[Project Name]
, m.[Task Name]
, m.[Client Product]
, m.[Project Number]
, m.[Date Worked]
, MONTH(m.[Date Worked]) as 'curMonth'
, YEAR(m.[Date Worked]) as 'Year'
, m.[User Full Name]
, m.[User Last Name]+', '+m.[User First Name] as Employee_NAme
, CASE WHEN m.[Task Name] = 'Retained Producer' THEN 'Producer' ELSE 'Designer' END as 'Title'
, RTRIM(xwrk_MC_Brd_Grp.salesmarketing) as 'SalesMarketing'
, m.[Service Description]
, m.[Actual Hours Worked]
, m.Comments 
from SQLWMJ.MOjo_prod.dbo.vReport_TimeDetail m
left outer join PJPROJ p on p.project = right(m.[Project Number],11)
JOIN xwrk_MC_Brd_Grp (nolock) ON p.pm_id01 = xwrk_MC_Brd_Grp.clientID AND p.pm_id02 = xwrk_MC_Brd_Grp.prodID
where [billing item id] = '12346'
and MONTH(m.[Date Worked]) between @BegMonth and @EndMonth
and YEAR(m.[Date Worked]) = @Year)a</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@BegMonth">
            <Value>=Parameters!BegMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndMonth">
            <Value>=Parameters!EndMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
