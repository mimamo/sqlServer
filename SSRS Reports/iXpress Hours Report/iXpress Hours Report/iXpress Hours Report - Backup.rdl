        <CommandText>-- access MOjo directly because my view isn't working
select
p.pm_id01 as 'ClientID'
, RTRIM(xwrk_MC_Brd_Grp.brand) as 'Brand'
, RTRIM(xwrk_MC_Brd_Grp.businessUnit) as 'BusinessUnit'
, RTRIM(xwrk_MC_Brd_Grp.subUnit) as 'SubUnit'
, 'iXpress' as 'Department'
, m.[Client Division]
, m.[Client Name]
, m.[Project Type]
, m.[Project Name]
, m.[Task Name]
, m.[Client Product]
, m.[Project Number]
, m.[Date Worked]
, m.[User Full Name]
, m.[Service Description]
, m.[Actual Hours Worked]
, m.Comments 
from SQLWMJ.MOJo_prod.dbo.vReport_TimeDetail m
left outer join PJPROJ p on p.project = right(m.[Project Number],11)
JOIN xwrk_MC_Brd_Grp (nolock) ON p.pm_id01 = xwrk_MC_Brd_Grp.clientID AND p.pm_id02 = xwrk_MC_Brd_Grp.prodID
where m.[Project Type] = 'IXP'
and m.[Service Description] like '%retained%'
and m.[Date Worked] between @dStart and @dEnd
order by xwrk_MC_Brd_Grp.businessUnit desc, m.[User Full Name], m.[Client Product], m.[Project Number]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@dStart">
            <Value>=Parameters!dStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dEnd">
            <Value>=Parameters!dEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="dStart">
      <DataType>String</DataType>
      <Prompt>d Start</Prompt>
    </ReportParameter>
    <ReportParameter Name="dEnd">
      <DataType>String</DataType>
      <Prompt>d End</Prompt>
    </ReportParameter>
  </ReportParameters>
</Report>